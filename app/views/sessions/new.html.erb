<%= render 'static/sign_in_with_google' %>

<%= form_for :user, url: { controller: "sessions", action: "new"}, method: :post  do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br><br>

  <%= f.submit "Log In" %>
<% end %>
